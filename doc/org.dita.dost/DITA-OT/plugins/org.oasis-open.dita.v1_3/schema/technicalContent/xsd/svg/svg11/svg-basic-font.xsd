  </xs:attributeGroup>
  <xs:element name="vkern">
    <xs:complexType>
      <xs:attributeGroup ref="svg:attlist.vkern"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.vkern">
    <xs:attributeGroup ref="xml:SVG.Core.attrib"/>
    <xs:attribute name="u1"/>
