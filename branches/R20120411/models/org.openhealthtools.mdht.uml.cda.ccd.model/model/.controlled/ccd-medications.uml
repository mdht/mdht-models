      <general xmi:type="uml:Class" href="../../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_p6Zs8EcZEd6besF9iFIJkQ"/>
    </generalization>
    <ownedAttribute xmi:id="_BAMU0D6tEeCAHY6aowfd8Q" name="id" visibility="private" aggregation="composite">
      <ownedComment xmi:id="_XjNZUD6uEeCAHY6aowfd8Q">
        <body>uniquely represents a particular kind of product</body>
      </ownedComment>
