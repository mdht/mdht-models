      <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_YcM8o1axEeCddY1UIvGtQg"/>
      <redefinedProperty href="../../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_GsU1UErtEd6FXKclYGEDCw"/>
    </ownedAttribute>
