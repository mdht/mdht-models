            <language>OCL</language>
            <body>self.getObservations()->select(observation : cda::Observation | not observation.oclIsUndefined() and observation.oclIsKindOf(toc::EpisodeObservation))->asSequence()->first().oclAsType(toc::EpisodeObservation)</body>
          </specification>
        </ownedRule>
        <ownedParameter xmi:id="_bBmd4FTKEeCxKs3JeVuz3w" type="_Vf4wgFTEEeC0DuxL20wbJg" direction="return">
