      <ownedRule xmi:id="_bBNcUFTKEeCxKs3JeVuz3w" name="ConditionId" constrainedElement="_VeS1EFTEEeC0DuxL20wbJg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_bBNcUVTKEeCxKs3JeVuz3w">
          <language>OCL</language>
          <body>(self.id->isEmpty() or self.id->exists(element | element.isNullFlavorUndefined())) implies (not self.id->isEmpty())</body>
