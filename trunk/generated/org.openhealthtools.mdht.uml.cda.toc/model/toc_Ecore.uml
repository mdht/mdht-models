      <ownedOperation xmi:id="_bBl20FTKEeCxKs3JeVuz3w" name="getEpisodeObservation" isQuery="true" bodyCondition="_bBmd4lTKEeCxKs3JeVuz3w">
        <ownedRule xmi:id="_bBmd4lTKEeCxKs3JeVuz3w" name="body" constrainedElement="_bBl20FTKEeCxKs3JeVuz3w">
