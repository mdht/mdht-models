      </specification>
    </ownedRule>
    <ownedRule xmi:id="_2Ma4ERSVEeGGzu7fmW9pXw" name="subscriberDateOfBirth" constrainedElement="_2Ma4BxSVEeGGzu7fmW9pXw">
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_2Ma4EhSVEeGGzu7fmW9pXw">
