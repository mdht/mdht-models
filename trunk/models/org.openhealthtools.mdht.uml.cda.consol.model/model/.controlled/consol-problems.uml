    <ownedAttribute xmi:id="_VEjJAFfdEeCb0LEisK_hTQ" name="statusCode" visibility="private">
      <type xmi:type="uml:Class" href="../../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_iYn74D55Ed6cla2oXsPtlA"/>
