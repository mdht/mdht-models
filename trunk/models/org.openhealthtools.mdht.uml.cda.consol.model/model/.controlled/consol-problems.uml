  <uml:Class xmi:id="_VD4aoFfdEeCb0LEisK_hTQ" name="Condition" clientDependency="_VF5z41fdEeCb0LEisK_hTQ _VF6a8FfdEeCb0LEisK_hTQ _VF6a8VfdEeCb0LEisK_hTQ _VF6a8lfdEeCb0LEisK_hTQ">
    <eAnnotations xmi:id="_VD7d8FfdEeCb0LEisK_hTQ" source="sourceClass">
      <references xmi:type="uml:Class" href="../../../org.openhealthtools.mdht.uml.cda.hitsp.model/model/.controlled/hitsp-problemList.uml#_GgvgUDxbEd6JwqSGecoePg"/>
    </eAnnotations>
    <ownedComment xmi:id="_VF5z4FfdEeCb0LEisK_hTQ">
      <body>A condition is a clinical statement that a clinician is particularly concerned about and wants to track. It has important patient management use cases (e.g. health records often present the problem list as a way of summarizing a patient's medical history).</body>
    </ownedComment>
    <ownedRule xmi:id="_VF3XoFfdEeCb0LEisK_hTQ" name="effectiveTimeLow">
      <constrainedElement xmi:type="uml:Property" href="../../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_hbddYEocEd6c3qsAxlFHaQ"/>
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_VF3XoVfdEeCb0LEisK_hTQ">
        <language>Analysis</language>
        <language>OCL</language>
        <body>SHALL contain exactly one [1..1] effectiveTime/low element</body>
        <body>not self.effectiveTime.low.oclIsUndefined()</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_dFDmAFlBEeCjtJnI04I27A" name="effectiveTimeHigh">
      <constrainedElement xmi:type="uml:Property" href="../../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_hbddYEocEd6c3qsAxlFHaQ"/>
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_dFDmAVlBEeCjtJnI04I27A">
        <language>Analysis</language>
        <language>OCL</language>
        <body>SHALL contain exactly one [1..1] effectiveTime/high element if statusCode@code=completed or aborted</body>
        <body>(self.statusCode.code = 'completed' or self.statusCode.code = 'aborted') implies not self.effectiveTime.high.oclIsUndefined()</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_dS6b0FlBEeCjtJnI04I27A" name="effectiveTimeHighProhibited">
      <constrainedElement xmi:type="uml:Property" href="../../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_hbddYEocEd6c3qsAxlFHaQ"/>
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_dS6b0VlBEeCjtJnI04I27A">
        <language>Analysis</language>
        <language>OCL</language>
        <body>SHALL NOT contain effectiveTime/high element if statusCode@code=active or suspended</body>
        <body>(self.statusCode.code = 'active' or self.statusCode.code = 'suspended') implies self.effectiveTime.high.oclIsUndefined()</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_VD4aoVfdEeCb0LEisK_hTQ" name="hasTreatingProvider">
      <constrainedElement xmi:type="uml:Property" href="../../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_LEB9oUxnEd6h-an5IXo-DA"/>
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_VD4aolfdEeCb0LEisK_hTQ">
        <language>Analysis</language>
        <language>OCL</language>
        <body>The treating provider or providers SHALL be recorded in a performer element.</body>
        <body>not self.performer->isEmpty()</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_VD4ao1fdEeCb0LEisK_hTQ" name="hasProviderId">
      <constrainedElement xmi:type="uml:Property" href="../../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_LEB9oUxnEd6h-an5IXo-DA"/>
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_VD4apFfdEeCb0LEisK_hTQ">
        <language>Analysis</language>
        <language>OCL</language>
        <body>SHALL contain at least one [1..*] performer/assignedEntity/id. The identifier of the treating provider SHALL be present in the id element. This identifier SHALL be the identifier of one of the providers listed in the healthcare providers module.</body>
        <body>self.performer->exists(p : cda::Performer2 | p.assignedEntity.id->size() > 0)</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_ywnaAF8UEeC6cuBpIAWIkQ" name="providerIDMatchesHeader" constrainedElement="_VD4ao1fdEeCb0LEisK_hTQ">
      <ownedComment xmi:id="_7xoHgF8UEeC6cuBpIAWIkQ">
        <body>Using a link between provider in the problems entry and the provider in the header eliminates duplicate information</body>
      </ownedComment>
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_3zKt4F8UEeC6cuBpIAWIkQ">
        <language>Analysis</language>
        <body>performer/assignedEntity/id SHALL link to a provider in the header under cda:ClinicalDocument/cda:documentationOf/cda:serviceEvent/cda:performer.</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_VD4apVfdEeCb0LEisK_hTQ" name="hasProviderTreatmentTime">
      <constrainedElement xmi:type="uml:Property" href="../../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_LEB9oUxnEd6h-an5IXo-DA"/>
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_VD4aplfdEeCb0LEisK_hTQ">
        <language>Analysis</language>
        <language>OCL</language>
        <body>The time over which this provider treated the condition MAY be recorded in the &amp;lt;time> element beneath the &amp;lt;performer> element</body>
        <body>self.performer->exists(p : cda::Performer2 | p.time.oclIsUndefined())</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_lNEK4F8TEeC6cuBpIAWIkQ" name="conditionEntryInversionInd" constrainedElement="_VD4aqFfdEeCb0LEisK_hTQ">
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_AOaRUF8UEeC6cuBpIAWIkQ">
        <language>Analysis</language>
        <body>SHALL contain exactly one [1..1] @inversionInd = &quot;false&quot;</body>
