    <ownedRule xmi:id="_K3gmcF-iEeCu6t3PoD5s8Q" name="episodeObsInversionInd" constrainedElement="_VEHEIFfdEeCb0LEisK_hTQ">
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_PW90UF-iEeCu6t3PoD5s8Q">
