  <uml:Class xmi:id="_2SVSlBSVEeGGzu7fmW9pXw" name="LanguageSpoken">
    <ownedRule xmi:id="_2SVSlRSVEeGGzu7fmW9pXw" name="usesLanguageCommunication" constrainedElement="_2SVSlBSVEeGGzu7fmW9pXw">
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_2SVSlhSVEeGGzu7fmW9pXw">
        <language>Analysis</language>
        <body>Languages spoken shall be recorded using the &amp;lt;languageCommunication&amp;gt; infrastructure class associated with the patient. The &amp;lt;languageCommunication&amp;gt; element describes the primary and secondary languages of communication for a person.</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_2SVSlxSVEeGGzu7fmW9pXw" name="signLanguage" constrainedElement="_2SVSlBSVEeGGzu7fmW9pXw">
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_2SVSmBSVEeGGzu7fmW9pXw">
        <language>Analysis</language>
        <body>Sign language is treated as a separate language.</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_2SVSmRSVEeGGzu7fmW9pXw" name="noProficiencyLevelCode" constrainedElement="_2SVSlBSVEeGGzu7fmW9pXw">
      <ownedComment xmi:id="_2SVSmhSVEeGGzu7fmW9pXw">
        <body>Judgments about language proficiency are subjective, and could have a negative impact on consumers.</body>
      </ownedComment>
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_2SVSmxSVEeGGzu7fmW9pXw">
        <language>Analysis</language>
        <language>OCL</language>
        <body>CDA allows for use of proficiencyLevelCode element, but this element SHOULD NOT be used.</body>
        <body>self.proficiencyLevelCode.oclIsUndefined()</body>
      </specification>
    </ownedRule>
    <generalization xmi:id="_2SbZMxSVEeGGzu7fmW9pXw">
      <general xmi:type="uml:Class" href="../../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_cHD44EcZEd6besF9iFIJkQ"/>
    </generalization>
    <ownedAttribute xmi:id="_2SVSnRSVEeGGzu7fmW9pXw" name="languageCode" visibility="private" aggregation="composite">
      <type xmi:type="uml:Class" href="../../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_iYn74D55Ed6cla2oXsPtlA"/>
      <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_2SVSnhSVEeGGzu7fmW9pXw" value="1"/>
      <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_2SVSnxSVEeGGzu7fmW9pXw" value="1"/>
    </ownedAttribute>
    <ownedAttribute xmi:id="_2SVSoBSVEeGGzu7fmW9pXw" name="modeCode" visibility="private" aggregation="composite">
      <ownedComment xmi:id="_2SVSoRSVEeGGzu7fmW9pXw">
        <body>Mode codes SHALL be appropriate to the type of language. Thus English, as spoken in the U.S. SHOULD use the code en-US and SHOULD only use mode codes for written and verbal communications. On the other hand, American Sign Language would be represented using the code sign-US, and would only use mode codes for signed communication.</body>
      </ownedComment>
      <type xmi:type="uml:Class" href="../../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_Z1EGIDxaEd6JwqSGecoePg"/>
      <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_2SVSohSVEeGGzu7fmW9pXw" value="1"/>
      <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_2SVSoxSVEeGGzu7fmW9pXw"/>
    </ownedAttribute>
  </uml:Class>
