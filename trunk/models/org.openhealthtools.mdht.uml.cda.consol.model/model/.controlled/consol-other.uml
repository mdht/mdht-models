    <ownedAttribute xmi:id="_2G7T0hSVEeGGzu7fmW9pXw" name="moodCode" visibility="private">
      <type xmi:type="uml:Enumeration" href="../../../org.openhealthtools.mdht.uml.hl7.vocab/model/vocab.uml#__zOpkkDcEd6xYZuu1VKCVw"/>
    </ownedAttribute>
    <ownedAttribute xmi:id="_2G5eqBSVEeGGzu7fmW9pXw" name="id" visibility="private" aggregation="composite">
      <ownedComment xmi:id="_2G5eqRSVEeGGzu7fmW9pXw">
        <body>This element shall be present. The root and extension attributes shall identify an element defined&#xD;
elsewhere in the same document.</body>
      </ownedComment>
      <type xmi:type="uml:Class" href="../../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_culQ8DxaEd6JwqSGecoePg"/>
      <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_2G5eqhSVEeGGzu7fmW9pXw" value="*"/>
      <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_2G5eqxSVEeGGzu7fmW9pXw"/>
    </ownedAttribute>
    <ownedAttribute xmi:id="_2G5ephSVEeGGzu7fmW9pXw" name="code" visibility="private" aggregation="composite">
      <ownedComment xmi:id="_2G5epxSVEeGGzu7fmW9pXw">
        <body>This element shall be present. It shall be valued when the internal reference is to element that has&#xD;
a &amp;lt;code> element, and shall have the same attributes as the &amp;lt;code> element in the act it&#xD;
references. If the element it references does not have a &amp;lt;code> element, then the nullFlavor&#xD;
attribute should be set to &quot;NA&quot;.</body>
      </ownedComment>
      <type xmi:type="uml:Class" href="../../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_VKl7EDxaEd6JwqSGecoePg"/>
    </ownedAttribute>
  </uml:Class>
