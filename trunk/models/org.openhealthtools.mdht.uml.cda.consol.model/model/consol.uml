        <redefinedProperty href="../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_NjnHQEb9Ed6besF9iFIJkQ"/>
        <defaultValue xmi:type="uml:LiteralString" xmi:id="_1QjCEQDkEeGkdOWPSEk2Sg" value="CLUSTER"/>
