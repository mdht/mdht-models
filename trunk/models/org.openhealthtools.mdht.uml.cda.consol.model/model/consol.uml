    <packagedElement xmi:type="uml:Association" xmi:id="_1WLI8QDkEeGkdOWPSEk2Sg" memberEnd="_1WKh4wDkEeGkdOWPSEk2Sg _1WLI8gDkEeGkdOWPSEk2Sg">
      <ownedEnd xmi:id="_1WLI8gDkEeGkdOWPSEk2Sg" type="_1WKh4ADkEeGkdOWPSEk2Sg" association="_1WLI8QDkEeGkdOWPSEk2Sg"/>
