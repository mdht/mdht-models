        <type xmi:type="uml:Enumeration" href="../../org.openhealthtools.mdht.uml.hl7.vocab/model/vocab.uml#__zOntkDcEd6xYZuu1VKCVw"/>
        <redefinedProperty href="../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_1DWqEEbnEd6besF9iFIJkQ"/>
        <defaultValue xmi:type="uml:LiteralString" xmi:id="_1MUpaQDkEeGkdOWPSEk2Sg" value="ACT"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="_1MaI8ADkEeGkdOWPSEk2Sg" name="moodCode" visibility="private" isReadOnly="true">
        <type xmi:type="uml:Enumeration" href="../../org.openhealthtools.mdht.uml.hl7.vocab/model/vocab.uml#__yeW3UDcEd6xYZuu1VKCVw"/>
        <defaultValue xmi:type="uml:InstanceValue" xmi:id="_1MaI8QDkEeGkdOWPSEk2Sg" name="INT">
          <type xmi:type="uml:Enumeration" href="../../org.openhealthtools.mdht.uml.hl7.vocab/model/vocab.uml#__yeW3UDcEd6xYZuu1VKCVw"/>
          <instance xmi:type="uml:EnumerationLiteral" href="../../org.openhealthtools.mdht.uml.hl7.vocab/model/vocab.uml#__yeW4EDcEd6xYZuu1VKCVw"/>
        </defaultValue>
      </ownedAttribute>
      <ownedAttribute xmi:id="_1MUpagDkEeGkdOWPSEk2Sg" name="code" visibility="private" aggregation="composite">
        <type xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_VKl7EDxaEd6JwqSGecoePg"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="_1MUpcADkEeGkdOWPSEk2Sg" name="text" visibility="private" aggregation="composite">
        <ownedComment xmi:id="_1MUpcQDkEeGkdOWPSEk2Sg">
          <body>The &amp;lt;text> element indicates the text of the comment. For CDA, this SHALL be represented as a &amp;lt;reference> element that points at the narrative portion of the document. The comment itself is&#xD;
not the act being coded, so it appears in the &amp;lt;text> of the &amp;lt;observation>, not as part of the&#xD;
&amp;lt;code>.</body>
        </ownedComment>
        <type xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_CowXkDxaEd6JwqSGecoePg"/>
        <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_1MUpcgDkEeGkdOWPSEk2Sg" value="1"/>
        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_1MUpcwDkEeGkdOWPSEk2Sg"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="_1MUpawDkEeGkdOWPSEk2Sg" name="statusCode" visibility="private" isReadOnly="true" aggregation="composite">
        <ownedComment xmi:id="_1MUpbADkEeGkdOWPSEk2Sg">
          <body>The code attribute of &amp;lt;statusCode> for all comments must be completed</body>
        </ownedComment>
        <type xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_iYn74D55Ed6cla2oXsPtlA"/>
        <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_1MUpbQDkEeGkdOWPSEk2Sg" value="1"/>
        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_1MUpbgDkEeGkdOWPSEk2Sg"/>
        <defaultValue xmi:type="uml:LiteralString" xmi:id="_1MUpbwDkEeGkdOWPSEk2Sg" value="completed"/>
      </ownedAttribute>
