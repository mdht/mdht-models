        <supplier xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda.ihe.model/model/.controlled/ihe-medications.uml#_sFGZUHGfEd6bf47ZOcft4w"/>
        <contract xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda.ihe.model/model/.controlled/ihe-medications.uml#_sFGZUHGfEd6bf47ZOcft4w"/>
      </substitution>
