        <supplier xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda.ihe.model/model/.controlled/ihe-medications.uml#_OeXF0HyTEd6FS-4xbA1fBQ"/>
        <contract xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda.ihe.model/model/.controlled/ihe-medications.uml#_OeXF0HyTEd6FS-4xbA1fBQ"/>
