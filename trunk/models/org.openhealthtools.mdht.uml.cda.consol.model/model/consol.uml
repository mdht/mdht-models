    <packagedElement xmi:type="uml:Association" xmi:id="_1WOzUwDkEeGkdOWPSEk2Sg" memberEnd="_1WOzUADkEeGkdOWPSEk2Sg _1WOzVADkEeGkdOWPSEk2Sg">
      <ownedEnd xmi:id="_1WOzVADkEeGkdOWPSEk2Sg" type="_1WKh4ADkEeGkdOWPSEk2Sg" association="_1WOzUwDkEeGkdOWPSEk2Sg"/>
