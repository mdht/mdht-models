    <packagedElement xmi:type="uml:Association" xmi:id="_1SN2AADkEeGkdOWPSEk2Sg" memberEnd="_1RdBAADkEeGkdOWPSEk2Sg _1SOdEADkEeGkdOWPSEk2Sg">
      <ownedEnd xmi:id="_1SOdEADkEeGkdOWPSEk2Sg" type="_1PzbMADkEeGkdOWPSEk2Sg" association="_1SN2AADkEeGkdOWPSEk2Sg"/>
