        <type xmi:type="uml:Enumeration" href="../../org.openhealthtools.mdht.uml.hl7.vocab/model/vocab.uml#__yeW3UDcEd6xYZuu1VKCVw"/>
        <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_1QjpIgDkEeGkdOWPSEk2Sg" value="1"/>
        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_1QjpIwDkEeGkdOWPSEk2Sg" value="1"/>
