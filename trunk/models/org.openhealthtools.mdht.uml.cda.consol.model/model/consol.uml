    <packagedElement xmi:type="uml:Association" xmi:id="_1MbXEADkEeGkdOWPSEk2Sg" memberEnd="_1MUpYgDkEeGkdOWPSEk2Sg _1Mb-IADkEeGkdOWPSEk2Sg">
      <ownedEnd xmi:id="_1Mb-IADkEeGkdOWPSEk2Sg" type="_1LZcUADkEeGkdOWPSEk2Sg" association="_1MbXEADkEeGkdOWPSEk2Sg"/>
    </packagedElement>
    <packagedElement xmi:type="uml:Class" xmi:id="_1Mb-IQDkEeGkdOWPSEk2Sg" name="MedicationType" clientDependency="_1MfBcADkEeGkdOWPSEk2Sg">
      <ownedComment xmi:id="_1Mb-IgDkEeGkdOWPSEk2Sg" annotatedElement="_1Mb-IQDkEeGkdOWPSEk2Sg">
        <body>A classification based on how the medication is&#xD;
marketed (e.g., prescription, over the counter drug)</body>
      </ownedComment>
      <generalization xmi:id="_1MeaYwDkEeGkdOWPSEk2Sg">
        <general xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda/model/cda.uml#_bJyIQDxYEd6JwqSGecoePg"/>
      </generalization>
      <substitution xmi:id="_1MfBcADkEeGkdOWPSEk2Sg" client="_1Mb-IQDkEeGkdOWPSEk2Sg">
