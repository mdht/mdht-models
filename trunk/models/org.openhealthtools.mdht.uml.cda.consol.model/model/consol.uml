    <packagedElement xmi:type="uml:Class" xmi:id="_1U6ksgDkEeGkdOWPSEk2Sg" name="AdvanceDirectiveReference" clientDependency="_1VAEQQDkEeGkdOWPSEk2Sg">
      <ownedRule xmi:id="_1U6kswDkEeGkdOWPSEk2Sg" name="hasConformanceTo" constrainedElement="_1U6ksgDkEeGkdOWPSEk2Sg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_1U6ktADkEeGkdOWPSEk2Sg">
          <language>Analysis</language>
          <body>An advance directive reference (templateId 2.16.840.1.113883.10.20.1.36) SHALL be represented with Observation / reference / ExternalDocument</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_1U6ktQDkEeGkdOWPSEk2Sg" name="hasReference" constrainedElement="_1U6ksgDkEeGkdOWPSEk2Sg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_1U6ktgDkEeGkdOWPSEk2Sg">
          <language>Analysis</language>
          <language>OCL</language>
          <body>The value for Observation / reference / @typeCode in an advance directive reference SHALL be 'REFR' 2.16.840.1.113883.5.1002 ActRelationshipType STATIC.</body>
          <body>self.reference.typeCode->forAll(tc : vocab::x_ActRelationshipExternalReference | tc =  vocab::x_ActRelationshipExternalReference::REFR)</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_1U6ktwDkEeGkdOWPSEk2Sg" name="hasURL" constrainedElement="_1U6ksgDkEeGkdOWPSEk2Sg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_1U6kuADkEeGkdOWPSEk2Sg">
          <language>Analysis</language>
          <language>OCL</language>
          <body>The URL of a referenced advance directive document MAY be present, and SHALL be represented in Observation / reference / ExternalDocument / text / reference. A &lt;linkHTML> element containing the same URL SHOULD be present in the associated CDA Narrative Block</body>
          <body>self.reference.externalDocument.text.reference->size() > 0</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_1U6kuQDkEeGkdOWPSEk2Sg" name="hasMIMEType" constrainedElement="_1U6ksgDkEeGkdOWPSEk2Sg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_1U6kugDkEeGkdOWPSEk2Sg">
          <language>Analysis</language>
          <language>OCL</language>
          <body>The MIME type of a referenced advance directive document MAY be present, and SHALL be represented in Observation / reference / ExternalDocument / text / @mediaType</body>
          <body>self.reference.externalDocument.text.mimeType.size() > 0</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_1U6kuwDkEeGkdOWPSEk2Sg" name="exchangePackage" constrainedElement="_1U6ksgDkEeGkdOWPSEk2Sg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_1U6kvADkEeGkdOWPSEk2Sg">
          <language>Analysis</language>
          <body>Where the value of Observation / reference / seperatableInd is &quot;false&quot;, the referenced advance directive document SHOULD be included in the CCD exchange package. The exchange mechanism SHOULD be based on Internet standard RFC 2557 &quot;MIME Encapsulation of Aggregate Documents, such as HTML (MHTML)&quot; (http://www.ietf.org/rfc/rfc2557.txt)</body>
        </specification>
      </ownedRule>
