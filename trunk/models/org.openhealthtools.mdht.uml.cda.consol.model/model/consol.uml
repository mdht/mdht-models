    <packagedElement xmi:type="uml:Class" xmi:id="_1J3yUADkEeGkdOWPSEk2Sg" name="ProblemStatusObservation" clientDependency="_1KHC4QDkEeGkdOWPSEk2Sg _1KHC4gDkEeGkdOWPSEk2Sg _1KHC4wDkEeGkdOWPSEk2Sg">
