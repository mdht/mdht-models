        <contract xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda.ihe.model/model/.controlled/ihe-medications.uml#_sFGZUHGfEd6bf47ZOcft4w"/>
      </substitution>
      <substitution xmi:id="_1jnIBgDkEeGkdOWPSEk2Sg" client="_1i4vQADkEeGkdOWPSEk2Sg">
        <supplier xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda.ihe.model/model/.controlled/ihe-medications.uml#_OY9BYHyTEd6FS-4xbA1fBQ"/>
        <contract xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda.ihe.model/model/.controlled/ihe-medications.uml#_OY9BYHyTEd6FS-4xbA1fBQ"/>
      </substitution>
      <substitution xmi:id="_1jnIBwDkEeGkdOWPSEk2Sg" client="_1i4vQADkEeGkdOWPSEk2Sg">
        <supplier xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda.hitsp/model/hitsp_Ecore.uml#_IjIO0DxbEd6JwqSGecoePg"/>
        <contract xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.cda.hitsp/model/hitsp_Ecore.uml#_IjIO0DxbEd6JwqSGecoePg"/>
      </substitution>
