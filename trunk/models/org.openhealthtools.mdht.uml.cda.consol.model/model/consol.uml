    <packagedElement xmi:type="uml:Association" xmi:id="_1JfX0QDkEeGkdOWPSEk2Sg" memberEnd="_1JL10ADkEeGkdOWPSEk2Sg _1JfX0gDkEeGkdOWPSEk2Sg">
      <ownedEnd xmi:id="_1JfX0gDkEeGkdOWPSEk2Sg" type="_1IvJ4ADkEeGkdOWPSEk2Sg" association="_1JfX0QDkEeGkdOWPSEk2Sg"/>
