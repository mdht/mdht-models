    <packagedElement xmi:type="uml:Association" xmi:id="_1SVKwQDkEeGkdOWPSEk2Sg" memberEnd="_1SPEKgDkEeGkdOWPSEk2Sg _1SVKwgDkEeGkdOWPSEk2Sg">
