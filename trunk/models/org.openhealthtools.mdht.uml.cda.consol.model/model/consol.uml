        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_1j2YkwDkEeGkdOWPSEk2Sg"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="_1j2_oADkEeGkdOWPSEk2Sg" name="maxDoseQuantity" visibility="private" aggregation="composite">
        <ownedComment xmi:id="_1j2_oQDkEeGkdOWPSEk2Sg">
          <body>defines a maximum or dose limit.&#xD;
This segment can repeat for more than one dose&#xD;
restriction</body>
        </ownedComment>
        <type xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_IZ0HUErsEd6FXKclYGEDCw"/>
