    <packagedElement xmi:type="uml:Association" xmi:id="_1ndGMADkEeGkdOWPSEk2Sg" memberEnd="_1ncfIADkEeGkdOWPSEk2Sg _1ndGMQDkEeGkdOWPSEk2Sg">
      <ownedEnd xmi:id="_1ndGMQDkEeGkdOWPSEk2Sg" type="_1my-4ADkEeGkdOWPSEk2Sg" association="_1ndGMADkEeGkdOWPSEk2Sg"/>
