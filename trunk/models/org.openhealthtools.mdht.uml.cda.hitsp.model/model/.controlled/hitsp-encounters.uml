    <ownedRule xmi:id="_KqGU0MN0EeCZQPUDKP01pg" name="orderToAdmitGuidance" constrainedElement="_x6SxkKsQEd-yFfcbpgNFIA">
      <ownedComment xmi:id="_z51R4AY2EeG7pO1MXdunbA">
