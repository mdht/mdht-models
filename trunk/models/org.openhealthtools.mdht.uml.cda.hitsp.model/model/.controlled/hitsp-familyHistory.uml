  <uml:Class xmi:id="_4Mh2UMigEd6mWrHPmrIIfQ" name="FamilyHistorySection">
    <ownedComment xmi:id="__W9csMigEd6mWrHPmrIIfQ" annotatedElement="_4Mh2UMigEd6mWrHPmrIIfQ">
      <body>&lt;p>&#xD;
    The Family History Section contains information about the genetic family members, to the extent that&#xD;
    they are known, the diseases they suffered from, their ages at death, and other relevant genetic information. &#xD;
&lt;/p></body>
    </ownedComment>
    <ownedRule xmi:id="_D5rukMgiEeCHk-lvWGDTdA" name="structuredFamilyHistoryConformance" constrainedElement="_4Mh2UMigEd6mWrHPmrIIfQ">
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_O2ah4MgiEeCHk-lvWGDTdA">
        <language>Analysis</language>
        <body>SHALL conform to IHE Coded Family History Section and SHALL contain a templateId element whose root attribute is 1.3.6.1.4.1.19376.1.5.3.1.3.15 when this section is conveying structured family history.</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="_vh_nQMgiEeCHk-lvWGDTdA" name="structuredFamilyHistoryModule" constrainedElement="_D5rukMgiEeCHk-lvWGDTdA">
      <specification xmi:type="uml:OpaqueExpression" xmi:id="_0gOb0MgiEeCHk-lvWGDTdA">
        <language>Analysis</language>
        <body>When providing structured Family History Information SHALL include entries conforming to the Family History module</body>
      </specification>
    </ownedRule>
