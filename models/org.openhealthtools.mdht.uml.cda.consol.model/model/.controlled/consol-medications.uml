  <CDA:CDATemplate xmi:id="_ypuToFmJEeCeYrW7M64v2A" base_Class="_ypsecFmJEeCeYrW7M64v2A" templateId="1.3.6.1.4.1.19376.1.5.3.1.4.7.1"/>
