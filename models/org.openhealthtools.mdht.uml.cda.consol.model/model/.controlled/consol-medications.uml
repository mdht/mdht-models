  <uml:Class xmi:id="_ypzzMFmJEeCeYrW7M64v2A" name="MedicationConditionalDose" clientDependency="_yp1BUlmJEeCeYrW7M64v2A _yp1BU1mJEeCeYrW7M64v2A _yp1BVFmJEeCeYrW7M64v2A _yp1BVVmJEeCeYrW7M64v2A _yp1BVlmJEeCeYrW7M64v2A">
