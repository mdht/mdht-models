        <defaultValue xmi:type="uml:LiteralString" xmi:id="_0cb-gwuuEeGvGIuLiiZuOg" value="EVN"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="_0cclkAuuEeGvGIuLiiZuOg" name="code" visibility="private" aggregation="composite">
        <type xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_VKl7EDxaEd6JwqSGecoePg"/>
        <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_0cclkQuuEeGvGIuLiiZuOg" value="1"/>
        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_0cclkguuEeGvGIuLiiZuOg" value="1"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="_0cfB0AuuEeGvGIuLiiZuOg" name="statusCode" visibility="private">
        <type xmi:type="uml:Class" href="../../org.openhealthtools.mdht.uml.hl7.datatypes/model/datatypes.uml#_iYn74D55Ed6cla2oXsPtlA"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="_0cg3AAuuEeGvGIuLiiZuOg" name="familyHistoryObservation" type="_0cheEAuuEeGvGIuLiiZuOg" association="_0c4qcAuuEeGvGIuLiiZuOg">
        <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_0cg3AQuuEeGvGIuLiiZuOg" value="*"/>
        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_0cg3AguuEeGvGIuLiiZuOg" value="1"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="_0cMuHAuuEeGvGIuLiiZuOg" name="problemStatusObservation" type="_0XWqOguuEeGvGIuLiiZuOg" association="_0caJUQuuEeGvGIuLiiZuOg">
        <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_0cMuHQuuEeGvGIuLiiZuOg" value="*"/>
