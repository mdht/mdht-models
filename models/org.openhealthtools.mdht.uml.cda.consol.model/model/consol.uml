        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_0XWqOQuuEeGvGIuLiiZuOg"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="_0YNl1QuuEeGvGIuLiiZuOg" name="healthStatusObservation" type="_0Wr72AuuEeGvGIuLiiZuOg" association="_0YNl1wuuEeGvGIuLiiZuOg">
        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_0YNl1guuEeGvGIuLiiZuOg"/>
      </ownedAttribute>
