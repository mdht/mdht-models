    <packagedElement xmi:type="uml:Association" xmi:id="_0YNl6guuEeGvGIuLiiZuOg" memberEnd="_0YNl5AuuEeGvGIuLiiZuOg _0YNl6wuuEeGvGIuLiiZuOg">
      <ownedEnd xmi:id="_0YNl6wuuEeGvGIuLiiZuOg" type="_0YNl3QuuEeGvGIuLiiZuOg" association="_0YNl6guuEeGvGIuLiiZuOg"/>
    </packagedElement>
