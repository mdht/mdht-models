    <packagedElement xmi:type="uml:Class" xmi:id="_0pgxOAuuEeGvGIuLiiZuOg" name="LanguageSpoken">
      <ownedRule xmi:id="_0pgxOQuuEeGvGIuLiiZuOg" name="usesLanguageCommunication" constrainedElement="_0pgxOAuuEeGvGIuLiiZuOg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_0pgxOguuEeGvGIuLiiZuOg">
          <language>Analysis</language>
          <body>Languages spoken shall be recorded using the &amp;lt;languageCommunication&amp;gt; infrastructure class associated with the patient. The &amp;lt;languageCommunication&amp;gt; element describes the primary and secondary languages of communication for a person.</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_0pgxOwuuEeGvGIuLiiZuOg" name="signLanguage" constrainedElement="_0pgxOAuuEeGvGIuLiiZuOg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_0pgxPAuuEeGvGIuLiiZuOg">
          <language>Analysis</language>
          <body>Sign language is treated as a separate language.</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_0pgxPQuuEeGvGIuLiiZuOg" name="noProficiencyLevelCode" constrainedElement="_0pgxOAuuEeGvGIuLiiZuOg">
        <ownedComment xmi:id="_0pgxPguuEeGvGIuLiiZuOg">
          <body>Judgments about language proficiency are subjective, and could have a negative impact on consumers.</body>
        </ownedComment>
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_0pgxPwuuEeGvGIuLiiZuOg">
          <language>Analysis</language>
          <language>OCL</language>
          <body>CDA allows for use of proficiencyLevelCode element, but this element SHOULD NOT be used.</body>
          <body>self.proficiencyLevelCode.oclIsUndefined()</body>
        </specification>
      </ownedRule>
