        <specification xmi:type="uml:OpaqueExpression" xmi:id="_0yTg0wuuEeGvGIuLiiZuOg">
          <language>Analysis</language>
          <language>OCL</language>
          <body>Contains clinical statements.</body>
          <body>not self.entry->isEmpty()</body>
        </specification>
      </ownedRule>
