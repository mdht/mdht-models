    <packagedElement xmi:type="uml:Association" xmi:id="_0yj_gAuuEeGvGIuLiiZuOg" memberEnd="_0yixZAuuEeGvGIuLiiZuOg _0yj_gQuuEeGvGIuLiiZuOg">
      <ownedEnd xmi:id="_0yj_gQuuEeGvGIuLiiZuOg" type="_0yfuEAuuEeGvGIuLiiZuOg" association="_0yj_gAuuEeGvGIuLiiZuOg"/>
