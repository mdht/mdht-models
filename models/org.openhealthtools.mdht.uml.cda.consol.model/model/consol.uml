    <packagedElement xmi:type="uml:Association" xmi:id="_0YmAUguuEeGvGIuLiiZuOg" memberEnd="_0WThVwuuEeGvGIuLiiZuOg _0YmAUwuuEeGvGIuLiiZuOg">
      <ownedEnd xmi:id="_0YmAUwuuEeGvGIuLiiZuOg" type="_0V7G0AuuEeGvGIuLiiZuOg" association="_0YmAUguuEeGvGIuLiiZuOg"/>
