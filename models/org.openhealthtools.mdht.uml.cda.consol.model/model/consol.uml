        <ownedComment xmi:id="_0kMMUguuEeGvGIuLiiZuOg">
          <body>UNIMPLEMENTABLE</body>
        </ownedComment>
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_0kMMUwuuEeGvGIuLiiZuOg">
          <language>Analysis</language>
          <body>title is valued with a case-insensitive language-insensitive text string containing &quot;procedures&quot;.</body>
        </specification>
      </ownedRule>
