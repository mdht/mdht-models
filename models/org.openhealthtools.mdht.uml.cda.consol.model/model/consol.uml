        </specification>
      </ownedRule>
      <ownedRule xmi:id="_0WThYwuuEeGvGIuLiiZuOg" name="displayNameCodeName" constrainedElement="_0WThYQuuEeGvGIuLiiZuOg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_0WThZAuuEeGvGIuLiiZuOg">
