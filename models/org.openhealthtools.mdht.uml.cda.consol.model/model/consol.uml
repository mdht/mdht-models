    <packagedElement xmi:type="uml:Association" xmi:id="_0a-l8QuuEeGvGIuLiiZuOg" memberEnd="_0a4fXQuuEeGvGIuLiiZuOg _0a-l8guuEeGvGIuLiiZuOg">
