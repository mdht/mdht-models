      <ownedRule xmi:id="_0vuG4AuuEeGvGIuLiiZuOg" name="hasNonXMLBodyText">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_0vuG4QuuEeGvGIuLiiZuOg">
          <language>Analysis</language>
          <language>OCL</language>
