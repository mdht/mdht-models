  </term:ValueSetVersion>
  <term:ValueSetVersion xmi:id="_3-Wtq-ovEeKrOYS9kpIZsQ" identifier="2.16.840.1.113883.1.11.20448" fullName="Purpose of Use" source="HL7" url="http://www.hl7.org/v3ballotarchive/v3ballot/html/infrastructure/vocabulary/vocabulary.html" definition="Reason for performing one or more operations on information, which may be permitted by source system’s security policy in accordance with one or more privacy policies and consent directives." base_Enumeration="_3-Wtd-ovEeKrOYS9kpIZsQ">
