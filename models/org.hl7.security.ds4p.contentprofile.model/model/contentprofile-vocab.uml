  </term:ValueSetVersion>
  <term:ValueSetVersion xmi:id="_3-WtweovEeKrOYS9kpIZsQ" identifier="2.16.840.1.113883.1.11.19930" source="HL7" url="http://www.hl7.org/v3ballotarchive/v3ballot/html/infrastructure/vocabulary/vocabulary.html" definition="This value set be used to specify the client's relationship to the Substitute Decision Maker – if one is involved in the creation of the privacy consent directive" type="Intensional" binding="Dynamic" base_Enumeration="_3-WtmeovEeKrOYS9kpIZsQ">
    <codeSystem href="pathmap://TERM_MODEL/model/CodeSystems.uml#_Si0RgOxyEeG7k4bbVdXbXw"/>
  </term:ValueSetVersion>
