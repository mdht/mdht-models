    <packagedElement xmi:type="uml:Class" xmi:id="_dYFEcGeZEeOu4KcTyYBqzA" name="ConfidentialitySecurityObservation">
      <ownedComment xmi:id="_dYFEcWeZEeOu4KcTyYBqzA">
        <body>This template is constrains the Security Observation to specify a
          &quot;confidentiality code&quot;. This template reuses the SecurityObservation
          implementing the HL7 Healthcare Security Classification (HCS) standard.</body>
      </ownedComment>
      <ownedRule xmi:id="_skumoOToEeuJLIg6tToucA" name="SecurityObservationTemplateId" constrainedElement="_dYFEcGeZEeOu4KcTyYBqzA">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_skumoeToEeuJLIg6tToucA">
          <language>OCL</language>
          <body>self.templateId->exists(id : datatypes::II | id.root = '2.16.840.1.113883.3.445.12')</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_skxp8OToEeuJLIg6tToucA" name="SecurityObservationCodeP" constrainedElement="_dYFEcGeZEeOu4KcTyYBqzA">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_skxp8eToEeuJLIg6tToucA">
          <language>OCL</language>
          <body>not self.code.oclIsUndefined()</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_skv0wOToEeuJLIg6tToucA" name="SecurityObservationCode" constrainedElement="_dYFEcGeZEeOu4KcTyYBqzA">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_skv0weToEeuJLIg6tToucA">
          <language>OCL</language>
          <body>not self.code.oclIsUndefined() and self.code.oclIsKindOf(datatypes::CD) and 
