          <language>OCL</language>
          <body>self.templateId->exists(id : datatypes::II | id.root = '2.16.840.1.113883.3.3251.1.7')</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_si1wQOToEeuJLIg6tToucA" name="MandatoryEntryAssignedAuthorAssignedAuthoringDevice" constrainedElement="_hBuuMOdREeKzv8U7OUnxlg">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_si1wQeToEeuJLIg6tToucA">
